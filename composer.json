{
    "name": "demency/laravel-friendships",
    "description": "This package gives Eloquent models the ability to manage their friendships.",
    "keywords": ["laravel", "friendships", "friend-system", "friends", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hootlex",
            "email": "hootlex@icloud.com"
        },
        {
            "name": "Ian Torres",
            "email": "iantorres@outlook.com"
        }
    ],
    "autoload-dev": {
        "classmap": [
            "vendor/laravel/laravel/tests/TestCase.php"
        ],
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "illuminate/auth": "^5.8|^6.0|^7.0",
        "illuminate/container": "^5.8|^6.0|^7.0",
        "illuminate/contracts": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "fzaninotto/faker": "~1.4",
        "codeclimate/php-test-reporter": "^0.3.2",
        "doctrine/dbal": "^2.5",
    },
    "autoload": {
        "psr-4": {
            "Demency\\Friendships\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Demency\\Friendships\\FriendshipsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
